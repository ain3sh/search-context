{
  "name": "@ain3sh/search-context",
  "version": "1.0.0",
  "description": "MCP server for semantic search over documentation using Gemini File Search API",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ain3sh/search-context.git"
  },
  "keywords": ["mcp", "mcp-server", "gemini", "semantic-search", "file-search"],
  "author": "ain3sh",
  "license": "MIT",
  "main": "dist/index.js",
  "bin": {
    "search-context": "dist/index.js"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsc",
    "prepare": "npm run build",
    "dev": "tsx watch src/index.ts",
    "start": "node dist/index.js",
    "clean": "rm -rf dist",
    "eval": "tsx evaluations/run-eval.ts"
  },
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "@google/genai": "^1.29.0",
    "@modelcontextprotocol/sdk": "^1.6.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/node": "^22.10.0",
    "fast-xml-parser": "^4.5.3",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2"
  }
}
